        <item name="android:textColor">@color/stripe_paymentsheet_title_text</item>
        <item name="android:lineSpacingExtra">9sp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="StripePaymentSheetTitleCustomAppStore">
        <item name="android:textSize">26sp</item>
        <item name="android:textColor">@color/appstore_stripe_color_white</item>
        <item name="android:layout_height">32dp</item>
        <item name="fontFamily">@font/font_roboto_regular</item>
    </style>
    <style name="StripePaymentSheetTotalLabel">
        <item name="android:textSize">13sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">@color/stripe_paymentsheet_header_text</item>
        <item name="android:lineSpacingExtra">5sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="StripePaymentSheetTotalLabelCustomAppstore">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">@color/stripe_paymentsheet_editetext_text_color</item>
        <item name="android:lineSpacingExtra">5sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="StripeToolBarStyle" parent="@style/ThemeOverlay.AppCompat">
        <item name="colorControlNormal">?attr/titleTextColor</item>
        <item name="actionMenuTextColor">?attr/titleTextColor</item>
        <item name="android:actionButtonStyle">@style/StripeActionButtonStyle</item>
    </style>
    <style name="StripeVerticalDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/appstore_stripe_color_transparent</item>
    </style>
    <style name="button_secondary" parent="@android:style/Widget.Button">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:height">60dp</item>
        <item name="android:textSize">@dimen/app_text_size_25_sp</item>
        <item name="android:background">@drawable/button_secondary</item>
        <item name="fontFamily">@font/font_roboto_medium</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <declare-styleable name="BecsDebitWidget">
        <attr format="string" name="companyName"/>
    </declare-styleable>
    <declare-styleable name="CardElement">
        <attr format="boolean" name="shouldShowPostalCode"/>
        <attr format="boolean" name="shouldRequirePostalCode"/>
        <attr format="boolean" name="shouldRequireUsZipCode"/>
    </declare-styleable>
    <declare-styleable name="CardInputView">
        <attr format="string" name="cardHintText"/>
        <attr format="color" name="cardTint"/>
        <attr format="color" name="cardTextErrorColor"/>
        <attr name="android:focusedByDefault"/>
    </declare-styleable>
    <declare-styleable name="StripeCardFormView">
        <attr format="enum" name="cardFormStyle">
            <enum name="standard" value="0"/>
            <enum name="borderless" value="1"/>
        </attr>
        <attr format="reference" name="backgroundColorStateList"/>
    </declare-styleable>
    <declare-styleable name="StripeCountryAutoCompleteTextInputLayout">
        <attr format="reference" name="countryAutoCompleteStyle"/>
        <attr format="reference" name="countryItemLayout"/>
    </declare-styleable>
</resources>